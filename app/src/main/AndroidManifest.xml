<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nagstud.adnan.mobileapp_1">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/nslogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/nslogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--Welcome Activity is the Welcome slide , it only runs once and passes on to MainActivity-->
        <activity android:name=".WelcomeActivity"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Teststatus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SyllabusActivity" />
        <activity android:name=".DownloadActivity" />
        <activity android:name=".SubscribedActivity" />
        <activity android:name=".EventsActivity" />
        <activity android:name=".AboutusActivity" />
        <activity android:name=".SingleItemView" />
        <activity android:name=".Answers_Activity1" />
        <activity android:name=".Answers_Activity2" />
        <activity android:name=".Answers_Activity3" />
        <activity android:name=".Answers_Activity4" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".getdata" />



    </application>

</manifest>